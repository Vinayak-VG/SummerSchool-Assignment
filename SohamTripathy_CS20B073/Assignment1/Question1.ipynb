    "import numpy as np\n",
    "\n",
    "n = 100\n",
    "\n",
    "y = np.random.randint(0, 2, (n,))\n",
    "y2 = np.random.rand(n)\n",
    "\n",
    "O_arr = y*(np.log2(y2)) + (1-y)*(np.log2(1-y2))\n",
    "\n",
    "print(-np.sum(O_arr, 0)/n)"
   ]
